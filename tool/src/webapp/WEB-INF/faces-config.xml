            <default-locale>en</default-locale>
        </locale-config>
    </application>

	<!-- Navigation Rules -->
    <navigation-rule>
		<from-view-id>/coursearchive/CourseArchiveItems.jsp</from-view-id>
		<navigation-case>
			<from-outcome>newItem</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveAddItem.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>updateItem</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveAddItem.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>deleteItems</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>*</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

    <navigation-rule>
		<from-view-id>/coursearchive/CourseArchiveAddItem.jsp</from-view-id>
		<navigation-case>
			<from-outcome>addedItem</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listItems</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>*</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

    <!-- Backing Bean -->
    <managed-bean>
        <description>CourseArchive Tool: CourseArchive Bean</description>
        <managed-bean-name>CourseArchiveBean</managed-bean-name>
        <managed-bean-class>org.sakaiproject.coursearchive.tool.jsf.CourseArchiveBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<description>CourseArchive Logic</description>
			<property-name>logic</property-name>
			<value>#{org_sakaiproject_coursearchive_logic_CourseArchiveLogic}</value>
		</managed-property>
		<managed-property>
			<description>External Logic</description>
			<property-name>externalLogic</property-name>
			<value>#{org_sakaiproject_coursearchive_logic_ExternalLogic}</value>
		</managed-property>
