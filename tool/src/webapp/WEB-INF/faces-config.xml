<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
	"http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<!-- This file generated by Sakai App Builder -AZ -->
<faces-config>
	<!-- Message Bundle -->
	<application>
		<message-bundle>messages</message-bundle>
		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>
	</application>

	<!-- Navigation Rules -->
	<navigation-rule>
		<from-view-id>/coursearchive/CourseArchiveItems.jsp</from-view-id>
		<navigation-case>
			<from-outcome>showItem</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveShowItem.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>deletedItems</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>*</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/coursearchive/CourseArchiveShowItem.jsp</from-view-id>
		<navigation-case>
			<from-outcome>editItem</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveEditItem.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>showRoster</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveShowRoster.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listItems</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>*</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/coursearchive/CourseArchiveEditItem.jsp</from-view-id>
		<navigation-case>
			<from-outcome>updatedItem</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listItems</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>showItem</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveShowItem.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>*</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/coursearchive/CourseArchiveShowRoster.jsp</from-view-id>
		<navigation-case>
			<from-outcome>showItem</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveShowItem.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listItems</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>*</from-outcome>
			<to-view-id>/coursearchive/CourseArchiveItems.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<!-- Backing Bean -->
	<managed-bean>
		<description>CourseArchive Tool: CourseArchive Bean</description>
		<managed-bean-name>CourseArchiveBean</managed-bean-name>
		<managed-bean-class>org.sakaiproject.coursearchive.tool.jsf.CourseArchiveBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<description>CourseArchive Logic</description>
			<property-name>logic</property-name>
			<value>#{org_sakaiproject_coursearchive_logic_CourseArchiveLogic}</value>
		</managed-property>
		<managed-property>
			<description>External Logic</description>
			<property-name>externalLogic</property-name>
			<value>#{org_sakaiproject_coursearchive_logic_ExternalLogic}</value>
		</managed-property>
	</managed-bean>

	<converter>
		<description>Converts line breaks to HTML br tags.</description>
		<converter-id>NewlineConverter</converter-id>
		<converter-class>org.sakaiproject.coursearchive.tool.jsf.NewlineConverter</converter-class>
	</converter>
</faces-config>
