<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.sakaiproject.coursearchive.model.CourseArchiveItem" table="course_archive_items">
		<id name="id" type="long">
			<generator class="native">
				<param name="sequence">course_archive_items_ID_SEQ</param>
			</generator>
		</id>
		<property name="title" type="string" length="255" not-null="true" />
		<property name="ownerId" type="string" length="255" not-null="true" />
		<property name="dateCreated" type="java.util.Date" />
		<property name="instructor" type="string" length="255" not-null="true"/>
		<property name="enrollment" type="integer" not-null="true"/>
	</class>
</hibernate-mapping>
